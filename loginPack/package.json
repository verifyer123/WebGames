    "babel-preset-stage-2": "^6.24.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-audio-player": "^0.9.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
